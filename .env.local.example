# Copy this file to .env.local and fill in your values

# Novu Configuration
NOVU_SECRET_KEY=your_novu_secret_key
NEXT_PUBLIC_NOVU_APPLICATION_IDENTIFIER=your_novu_app_id
NEXT_PUBLIC_NOVU_SUBSCRIBER_ID=your_subscriber_id

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: Override default Novu endpoints for self-hosted
# NOVU_API_URL=https://api.novu.co
# NOVU_DASHBOARD_HOST=https://dashboard.novu.co/
# NOVU_WEBSOCKET_URL=https://ws.novu.co